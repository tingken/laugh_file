package com.quark.api.auto.bean;

import android.media.Image;
import android.widget.RatingBar;

/**
 * Created by Administrator on 2016/5/21 0021.
 */
public class FavoritesInfo {

    private Image head;//

    private String text1;

    private String text2;

    private String text3;

    private RatingBar grade;//评分

    private String miles;

    private String FOOTBALL;

    private String date;

    public FavoritesInfo(Image head, String date, String FOOTBALL, String miles, RatingBar grade, String text3, String text2, String text1) {
        this.head = head;
        this.date = date;
        this.FOOTBALL = FOOTBALL;
        this.miles = miles;
        this.grade = grade;
        this.text3 = text3;
        this.text2 = text2;
        this.text1 = text1;
    }

    public Image getHead() {
        return head;
    }

    public void setHead(Image head) {
        this.head = head;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public String getFOOTBALL() {
        return FOOTBALL;
    }

    public void setFOOTBALL(String FOOTBALL) {
        this.FOOTBALL = FOOTBALL;
    }

    public String getMiles() {
        return miles;
    }

    public void setMiles(String miles) {
        this.miles = miles;
    }

    public RatingBar getGrade() {
        return grade;
    }

    public void setGrade(RatingBar grade) {
        this.grade = grade;
    }

    public String getText3() {
        return text3;
    }

    public void setText3(String text3) {
        this.text3 = text3;
    }

    public String getText2() {
        return text2;
    }

    public void setText2(String text2) {
        this.text2 = text2;
    }

    public String getText1() {
        return text1;
    }

    public void setText1(String text1) {
        this.text1 = text1;
    }

    @Override
    public String toString() {
        return "FavoritesInfo{" +
                "head=" + head +
                ", text1='" + text1 + '\'' +
                ", text2='" + text2 + '\'' +
                ", text3='" + text3 + '\'' +
                ", grade=" + grade +
                ", miles='" + miles + '\'' +
                ", FOOTBALL='" + FOOTBALL + '\'' +
                ", date='" + date + '\'' +
                '}';
    }
}
