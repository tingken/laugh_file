<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/newcourse_two"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include
        layout="@layout/main_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/qianhuise" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Session Length:" />

                <TextView
                    android:id="@+id/timelong"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:gravity="center|right"
                    android:text="" />

                <ImageView
                    android:id="@+id/timelongpic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="20dp"
                    android:src="@drawable/forward" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Session Rate ($):" />

                <EditText
                    android:id="@+id/rate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="Session Rate"
                    android:inputType="numberDecimal"
                    android:textSize="@dimen/fontsize_14sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Preferred Age of Students:" />

                <EditText
                    android:id="@+id/teachingage"
                    android:layout_width="0dp"
                    android:focusable="false"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="age 5 and up"
                    android:inputType="numberDecimal"
                    android:paddingRight="20dp"
                    android:textSize="@dimen/fontsize_14sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Experienced Year" />

                <TextView
                    android:id="@+id/teachingsince"
                    android:layout_width="0dp"
                    android:layout_height="45dp"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:gravity="center|right"
                    android:text="" />

                <ImageView
                    android:id="@+id/teachingsincepic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="20dp"
                    android:src="@drawable/forward" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/space_25"
                android:background="@color/beijing" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Travel To Sessions:" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:gravity="center|right"
                    android:text="" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="20dp">

                    <TextView
                        android:id="@+id/travelyes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/left_choose"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:text="YES"
                        android:textColor="@color/white" />

                    <TextView
                        android:id="@+id/travelno"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/right_nomal"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:text="NO"
                        android:textColor="@color/chengse" />

                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:id="@+id/travelview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:minHeight="50dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:text="Distance:" />

                    <TextView
                        android:id="@+id/distance"
                        android:layout_width="0dp"
                        android:layout_height="35dp"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1"
                        android:gravity="right|center"
                        android:hint="distance"
                        android:textSize="@dimen/fontsize_14sp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:minHeight="50dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:text="Travel Fee ($):" />

                    <EditText
                        android:id="@+id/tracfic_surcharge"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="Tracfic surcharge"
                        android:inputType="numberDecimal"
                        android:textSize="@dimen/fontsize_14sp" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/line_color" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginLeft="@dimen/space_20"
                    android:gravity="center"
                    android:text="Location:" />

                <LinearLayout
                    android:id="@+id/addressview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:minHeight="45dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/addresstext"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/space_20"
                        android:layout_marginRight="@dimen/space_10"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:text=""
                        android:hint="请选择位置"
                        android:textColor="@color/qianhuise" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:background="@drawable/forward" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:background="@color/danhuise"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="travel to sessions if you choose yes,means you are agress with travel terms"
                        android:textColor="@color/qianhuise" />

                </LinearLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Number of Extra Partners Per Session:" />

                <EditText
                    android:id="@+id/addpartner"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="center|right"
                    android:hint="Additional partner"
                    android:inputType="numberDecimal"
                    android:text=""
                    android:textSize="@dimen/fontsize_14sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Surcharge for Each Extra Partner($)" />

                <EditText
                    android:id="@+id/surcharge"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="center|right"
                    android:hint="Surcharge for each"
                    android:inputType="numberDecimal"
                    android:text=""
                    android:textSize="@dimen/fontsize_14sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:minHeight="50dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Discount by course" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/space_10"
                    android:layout_weight="1"
                    android:gravity="center|right"
                    android:text="" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/line_color" />

            <LinearLayout
                android:id="@+id/ByaccountView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/discount_one"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.quark.swipedelete.SwipeLayout
                        android:id="@+id/swipeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="45dp">

                        <TextView
                            android:id="@+id/discount_one_text"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:drawableRight="@drawable/address_right"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:text="one-time buy 3 course get xxx off" />

                        <include layout="@layout/layout_delete" />
                    </com.quark.swipedelete.SwipeLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/discount_two"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.quark.swipedelete.SwipeLayout
                        android:id="@+id/swipeLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="45dp">

                        <TextView
                            android:id="@+id/discount_two_text"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:drawableRight="@drawable/address_right"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:text="one-time buy 5 course get xxx off" />

                        <include layout="@layout/layout_delete" />
                    </com.quark.swipedelete.SwipeLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/discount_none"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.quark.swipedelete.SwipeLayout
                        android:id="@+id/swipeLayout3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/discount_three_text"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:drawableRight="@drawable/address_right"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:text="one-time buy 10 course get xxx off" />

                        <include layout="@layout/layout_delete" />
                    </com.quark.swipedelete.SwipeLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/BytotalView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/discount_one_total"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.quark.swipedelete.SwipeLayout
                        android:id="@+id/swipeLayout_total"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/discount_one_text_total"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:drawableRight="@drawable/address_right"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:text="none" />

                        <include layout="@layout/layout_delete" />
                    </com.quark.swipedelete.SwipeLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/discount_two_total"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.quark.swipedelete.SwipeLayout
                        android:id="@+id/swipeLayout2_total"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/discount_two_text_total"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:drawableRight="@drawable/address_right"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:text="none" />

                        <include layout="@layout/layout_delete" />
                    </com.quark.swipedelete.SwipeLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/discount_none_total"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.quark.swipedelete.SwipeLayout
                        android:id="@+id/swipeLayout3_total"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/discount_three_text_total"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:drawableRight="@drawable/address_right"
                            android:gravity="center_vertical"
                            android:paddingLeft="20dp"
                            android:paddingRight="20dp"
                            android:text="none" />

                        <include layout="@layout/layout_delete" />
                    </com.quark.swipedelete.SwipeLayout>
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/space_10"
                android:text="2/3" />

            <Button
                android:id="@+id/travel_to_sessions"
                style="@style/course_Button"
                android:text="@string/next" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>