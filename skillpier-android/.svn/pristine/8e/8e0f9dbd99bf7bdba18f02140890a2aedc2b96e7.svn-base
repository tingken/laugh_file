    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:gravity="center">

        <TextView
            android:id="@+id/tosearch"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginLeft="20dp"
            android:layout_weight="1"
            android:background="@drawable/edit_while_stylesearch"
            android:drawableLeft="@drawable/search_2"
            android:drawablePadding="8dp"
            android:singleLine="true"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:text="find your private coach or lesson"
            android:textColorHint="@color/yellow" />

        <ImageView
            android:id="@+id/msg"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:src="@drawable/message_2" />
    </LinearLayout>

    <View
        android:background="@color/qianhuise"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_1"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="暂无数据"
        android:visibility="gone"
        android:gravity="center"
        android:id="@+id/nodata"
        android:textSize="@dimen/fontsize_18sp"
        />

    <LinearLayout
        android:id="@+id/kecheng"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="horizontal">

        <ListView
            android:id="@+id/category"
            android:layout_width="0dp"
            android:scrollbars="none"
            android:background="@color/leftlist"
            android:layout_height="match_parent"
            android:layout_weight="1.3"></ListView>
        <View
            android:background="@color/dandanhuang"
            android:layout_width="0.5dp"
            android:layout_height="match_parent"/>

        <FrameLayout
            android:id="@+id/fragment_container"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3.0" />
    </LinearLayout>


</LinearLayout>