public class VerifyVisaPaymentsRequest {
    public String url = "/app/Pays/verifyVisaPayments";
    public String method = "get";
    private String orders_ids;//订单id
    private String amount;//支付金额
    private String dataValue;//app发起支付时visa返回的 dataValue
    private String dataDescriptor;//app发起支付时visa返回的 dataDescriptor

    public void setUrl(String url) {
        this.url = url;
    }

    public String getUrl() {
        return this.url;
    }

    public void setMethod(String method) {
        this.method = method;
    }

    public String getMethod() {
        return this.method;
    }

    public String getOrders_ids() {
        return orders_ids;
    }

    public void setOrders_ids(String orders_ids) {
        this.orders_ids = orders_ids;
    }

    public void setAmount(String amount) {
        this.amount = amount;
    }

    public String getAmount() {
        return this.amount;
    }

    public String getDataValue() {
        return dataValue;
    }

    public void setDataValue(String dataValue) {
        this.dataValue = dataValue;
    }

    public String getDataDescriptor() {
        return dataDescriptor;
    }

    public void setDataDescriptor(String dataDescriptor) {
        this.dataDescriptor = dataDescriptor;
    }
}
