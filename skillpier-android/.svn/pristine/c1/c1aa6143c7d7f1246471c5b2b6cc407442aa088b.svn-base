   }
   public String getTeaching_environment(){
     return this.teaching_environment;
   }

   public void setTeaching_attitude(String teaching_attitude){
     this.teaching_attitude = teaching_attitude;
   }
   public String getTeaching_attitude(){
     return this.teaching_attitude;
   }

   public void setApp_sign(String app_sign){
     this.app_sign = app_sign;
   }
   public String getApp_sign(){
     return this.app_sign;
   }

   public void setInvoke(String invoke){
