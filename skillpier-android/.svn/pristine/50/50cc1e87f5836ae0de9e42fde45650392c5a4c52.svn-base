<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include
        layout="@layout/main_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:orientation="vertical">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/notyet" />

        <!-- <TextView
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_marginTop="30dp"
             android:gravity="center"
             android:text=""
             android:textColor="@color/qianhuise"
             android:textSize="@dimen/text_size_15" />

         <TextView
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_marginTop="@dimen/space_10"
             android:gravity="center"
             android:text="@string/two"
             android:textColor="@color/qianhuise"
             android:textSize="@dimen/text_size_15" />-->

        <TextView
            android:id="@+id/tips"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/space_30"
            android:layout_marginRight="@dimen/space_30"
            android:layout_marginTop="@dimen/space_30"
            android:gravity="center"
            android:text="@string/three"
            android:textColor="@color/qianhuise"
            android:textSize="@dimen/text_size_15" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_35"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/service_phone"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginLeft="@dimen/space_20"
                android:layout_weight="1"
                android:background="@drawable/text_left"
                android:gravity="center"
                android:paddingBottom="@dimen/space_8"
                android:paddingLeft="@dimen/space_10"
                android:paddingRight="@dimen/space_10"
                android:paddingTop="@dimen/space_8"
                android:text="@string/four"
                android:textColor="@color/qianhuise"
                android:textSize="@dimen/fontsize_14sp" />

            <TextView
                android:id="@+id/new_renzheng_tv"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginLeft="@dimen/space_5"
                android:layout_marginRight="@dimen/space_20"
                android:layout_weight="1"
                android:background="@drawable/text_right"
                android:gravity="center"
                android:paddingBottom="@dimen/space_8"
                android:paddingTop="@dimen/space_8"
                android:text="@string/five"
                android:textColor="@color/white" />

        </LinearLayout>

        <TextView
            android:id="@+id/tip2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/space_30"
            android:layout_marginRight="@dimen/space_30"
            android:layout_marginTop="@dimen/space_25"
            android:gravity="center"
            android:text=" you have xx times to reapply, if you have any question, please contact us"
            android:textColor="@color/qianhuise" />

    </LinearLayout>

</LinearLayout>